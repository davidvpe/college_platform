{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "plugin:vue/vue3-essential",
  "overrides": [],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "no-console": "error",
    "no-var": "error"
  }
}
